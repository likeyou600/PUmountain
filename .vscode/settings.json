{
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/.history/**"
    ],
    "bladeFormatter.format.wrapLineLength": 180,
    "blade.format.enable": true,
    "editor.defaultFormatter": "onecentlin.laravel-blade",
    "[css]": {
        "editor.defaultFormatter": "lonefy.vscode-JS-CSS-HTML-formatter"
    }
}